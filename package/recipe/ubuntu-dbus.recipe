# recipe file

[name]
dbus

[package]
dbus
gconf-defaults-service
libgnome2-common

[action]
if [ ! -e /var/lib/dbus/machine-id ]; then dbus-uuidgen > /var/lib/dbus/machine-id; fi; 

[binary]

/usr/bin/dbus-launch  
/usr/bin/dbus-monitor  
/usr/bin/dbus-send
/bin/dbus-daemon
/bin/dbus-cleanup-sockets
/bin/dbus-uuidgen

[data]
/etc/dbus-1
/usr/share/dbus-1
/lib/dbus-1.0
/var/run/dbus

/usr/lib/libgconf2-4
/var/lib/gconf
/usr/share/gconf
/etc/gconf

[config]

/var/lib/dbus/machine-id
